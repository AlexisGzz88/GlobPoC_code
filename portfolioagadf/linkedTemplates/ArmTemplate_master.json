{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "portfolioagadf"
		},
		"ls_adls_portfolioag_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_portfolioag'"
		},
		"ls_mattel_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_mattel_databricks'"
		},
		"ls_adls_portfolioag_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://portfolioagdl.dfs.core.windows.net/"
		},
		"ls_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1127-042426-bnae40f9"
		},
		"ls_http_file_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"ls_http_multiplefiles_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"ls_mattel_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1127-042426-bnae40f9"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "portfolioagadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_portfolioag_accountKey": {
						"value": "[parameters('ls_adls_portfolioag_accountKey')]"
					},
					"ls_mattel_databricks_accessToken": {
						"value": "[parameters('ls_mattel_databricks_accessToken')]"
					},
					"ls_adls_portfolioag_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_portfolioag_properties_typeProperties_url')]"
					},
					"ls_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_http_file_properties_typeProperties_url": {
						"value": "[parameters('ls_http_file_properties_typeProperties_url')]"
					},
					"ls_http_multiplefiles_properties_typeProperties_url": {
						"value": "[parameters('ls_http_multiplefiles_properties_typeProperties_url')]"
					},
					"ls_mattel_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_mattel_databricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "portfolioagadf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'portfolioagadf_ArmTemplate_0')]"
			]
		}
	]
}